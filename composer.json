{
    "name": "towoju5/laravel-referral",
    "description": "Laravel package for a referral system",
    "keywords": [
        "jijunair",
        "towoju",
        "laravel",
        "referral",
        "affiliate",
        "package",
        "laravel-referral",
        "referral-system",
        "user-referral"
    ],
    "homepage": "https://github.com/towoju5/laravel-referral",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel",
            "email": "hello@woju.com.ng"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": ">=8.0",
        "illuminate/auth": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jijunair\\LaravelReferral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jijunair\\LaravelReferral\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jijunair\\LaravelReferral\\Providers\\ReferralServiceProvider"
            ]
        }
    }
}
